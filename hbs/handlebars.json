	// overlay
	"overlay partials from modules/overlay/open" : {
		"prefix": "proo",
		"body": [
			"{{> modules/overlay/open",
				"\tid='${1:overlay_id}'",
				"\tclass='${2:overlay_class}'",
				"\ttitle='${3:overlay_title}'",
				"\toverlay_type='${4|type-1,type-2|}'",
			"}}"
		],
		"description": "partial overlay open from 'modules/overlay'"
	},
	"Overlay partials from modules/overlay/close" : {
		"prefix": "proc",
		"body": "{{> modules/overlay/close}}",
