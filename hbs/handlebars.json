			"}}"
		],
		"description": "partial textarea from 'modules/form'"
	},
	"form field input partials from modules/form" : {
		"prefix": "prmffi",
		"body": [
			"{{> modules/form field_type='form-field-input'",
				"\ttype='${1|text,password,number,date,email,button,reset,submit,radio,checkbox|}'",
				"\tname='${2:name}'",
				"\t${3:mandatory='yes'}",
				"\tfield_label='${4:label}'",
				"\t${5:placeholder='${6:Input Type Nuber}'}",
			"}}"
		],
		"description": "partial form field input from 'modules/form'"
	},
	"form field select partials from modules/form" : {
		"prefix": "prmffs",
		"body": [
			"{{> modules/form field_type='form-field-select'",
				"\ttypeOf='${1|array,obj|}'",
				"\tname='${2:name}'",
				"\tselect_values='${3:array_or_object}'",
				"\t${4:mandatory='yes'}",
				"\tfield_label='${5:label}'",
				"\t${6:placeholder='${7:Select}'}",
				"\t${8:opt_label='${9:label}'}",
				"\t${10:opt_value='${11:url}'}",
			"}}"
		],
		"description": "partial Selelct from 'modules/form'"
	},
