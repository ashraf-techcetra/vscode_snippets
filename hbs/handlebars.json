{
	"form layout" : {
		"prefix": "fl",
		"body": [
			"<form action='' method='post' class='check-form'>",
				"\t<div class='form-layout'>",
					"\t\t$0",
				"\t</div>",
			"</form>"
		],
		"description": "form and form layout"
	},
	"Form layout row" : {
		"prefix": "flr",
		"body": [
			"<div class='form-row'>",
				"\t$0",
			"</div>"
		],
		"description": "form layout row"
	},
	"Form layout row with field" : {
		"prefix": "flrf",
		"body": [
			"<div class='form-row'>",
				"\t<div class='form-field'>",
					"\t\t$0",
				"\t</div>",
			"</div>"
		],
		"description": "form layout row"
	},
	"Form layout field" : {
		"prefix": "flf",
		"body": [
			"<div class='form-field'>",
				"\t$0",
			"</div>"
		],
		"description": "form layout form field"
	},
	// partials 
	"Basic Partials" :{
		"prefix": "pr",
		"body": [
			"{{> ${1:/path/to/partials}",
				"\t${0:partials options}",
			"}}"
		],
		"description": "Handlebars basic partials code"
	},
	"Partials from layouts" : {
		"prefix": "prl",
		"body": [
			"{{> layout/${1|head,header,footer,nav|}",
				"\t${0:partials options}",
			"}}"
		],
		"description": "Choose partials from layout folder"
	},
	"Partials from modules" : {
		"prefix": "prm",
		"body": [
			"{{> modules/${1|breadcrumbs,component,form,icons,loader,select,overlay/open,overlay/close|}",
				"\t${0:partials options}",
			"}}"
		],
		"description": "Choose partials from modules folder"
	},
	"input Partials from modules/form " : {
		"prefix": "prmfi",
		"body": [
			"{{> modules/form field_type='input'",
				"\ttype='${1|text,password,number,date,email,button,reset,submit,radio,checkbox|}'",
				"\tname='${2:field_name}'",
				"\t${3:class='${4:input_class}'}",
				"\t${5:placeholder='${6:input_placeholder}'}",
				"\t${7:id='${8:input_id}'}",
			"}}"
		],
		"description": "partial input from 'modules/form'"
	},
	"label partials from modules/form" : {
		"prefix": "prmflb",
		"body": [
			"{{> modules/form field_type='label'",
				"\tlabel_for=${1:label_for}",
				"\t${2:label_class=${3:label_class}}",
				"\tlabel_id=${4:label_id}",
				"\tcontext=${5:context}",
				"}}"
		],
		"description": "partial input checkbox from 'modules/form'"
	},
	"errors partials from modules/form" : {
		"prefix": "prmfe",
		"body": [
			"{{> modules/form field_type='errors' errors=${1:errors_arrary} }}"
		],
		"description": "partial errors from 'modules/form'"
	},
	"input-radio partials from modules/form" : {
		"prefix": "prmfir",
		"body": [
			"{{> modules/form field_type='input-radio' name='${1:name}' id='${2:id}' field_label='${3:label}' }}"
		],
		"description": "partial input radio from 'modules/form'"
	},
	"input-checkbox partials from modules/form" : {
		"prefix": "prmfic",
		"body": [
			"{{> modules/form field_type='input-checkbox' name='${1:name}' id='${2:id}' field_label='${3:label}'}}"
		],
		"description": "partial input checkbox from 'modules/form'"
	},
	"input-file partials from modules/form" : {
		"prefix": "prmfif",
		"body": [
			"{{> modules/form field_type='input-file'",
